{
    "Kestrel": {
        "Limits": {
            "MaxRequestBodySize": 5242880
        }
    },
    "Logging": {
        "LogLevel": {
            "Default": "Warning"
        }
    },
    "ConnectionStrings": {
        "Database": "Data Source=[HOSTNAME];Initial Catalog=GestioneSagre-Utility;User ID=[USERNAME];Password=[PASSWORD]",
        "ReadDatabase": "Data Source=[HOSTNAME];Initial Catalog=GestioneSagre-Read-Utility;User ID=[USERNAME];Password=[PASSWORD]",
        "RabbitMq": "amqp://[USERNAME]:[PASSWORD]@[HOSTNAME]:5672"
    },
    "RabbitMqSettings": {
        "ApplicationName": "gestioneSagre-utility",
        "QueuePrefetchCount": 1
    },
    "Smtp": {
        "Host": "smtp.example.org",
        "Port": 25,
        "Security": "StartTls",
        "Username": "Username del server SMTP",
        "Password": "Password del server SMTP",
        "Sender": "Gestione Sagre <noreply@example.org>",
        "MaxSenderCount": 10
    }
}